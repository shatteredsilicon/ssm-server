{
  "name": "install-wizard",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build install-wizard --base-href '/password-page/' --output-hashing=all --aot --build-optimizer",
    "build:prod": "ng b install-wizard --base-href '/password-page/' --prod --output-hashing=all --build-optimizer",
    "build:ami": "ng b --base-href --progress  --aot --output-path dist/ami/ --configuration=ami --output-hashing=all --build-optimizer",
    "build:ovf": "ng b --base-href --progress --aot --output-path dist/ovf/ --configuration=ovf --output-hashing=all --build-optimizer",
    "build:docker": "ng b --base-href --progress --aot --output-path dist/docker/ --configuration=docker --output-hashing=all --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.3",
    "@angular/common": "^13.0.3",
    "@angular/compiler": "^13.0.3",
    "@angular/core": "^13.0.3",
    "@angular/forms": "^13.0.3",
    "@angular/platform-browser": "^13.0.3",
    "@angular/platform-browser-dynamic": "^13.0.3",
    "@angular/router": "^13.0.3",
    "bootstrap": "^5.1.3",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.3",
    "@angular/cli": "^13.0.3",
    "@angular/compiler-cli": "^13.0.3",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.55",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "typescript": "~4.4.3"
  }
}
